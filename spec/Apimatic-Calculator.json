              examples:
                Active Bank Account:
                  value:
                    token: dest-efacd12b-a86e-4f44-bbea-927955ec1634
                    status: ACTIVE
                    createdOn: '2020-02-21T22:00:00Z'
                    bankAccountOwnershipType: PERSONAL
                    type: CHECKING
                    fields:
                      - key: BANK_ACH_ABA
                        value: '012346789'
                      - key: BANK_BBAN
                        value: '987654321'
                    bankCurrency: USD
                    bankCountry: US
                    description: Personal checking account
                    links:
                      - params:
                          rel: self
                        href: string
